/* This is the IOEX/KBD IC present on GCS AIC */
&CHARGER_I2C {
	it8801_mfd: it8801@39 {
		compatible = "ite,it8801-mfd";
		reg = <0x39>;
	};
};

#include <ite/it8801-common-cfg.dtsi>

/ {
	aliases {
		i2c-2 = &i2c2;
	};

	named-i2c-ports {
		compatible = "named-i2c-ports";
		i2c_ish_sensor: ish_sensor {
			i2c-port = <&i2c2>;
			enum-names = "I2C_PORT_SENSOR";
		};
	};
};

&i2c2 {
	status = "okay";
	clock-frequency = <I2C_BITRATE_FAST>;
};

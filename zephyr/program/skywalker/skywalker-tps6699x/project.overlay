#include "../skywalker/project.overlay"

/delete-node/ &pdc_power_p0;
/delete-node/ &pdc_power_p1;

&i2c1_0 {
	pdc_power_p0: tps6699-pdc@20 {
		compatible = "ti,tps6699-pdc";
		reg = <0x20>;
		irq-gpios = <&gpioe 1 GPIO_ACTIVE_LOW>;
		ccd;
		port-index-on-chip = <0x1>;
	};
	pdc_power_p1: tps6699-pdc@21 {
		compatible = "ti,tps6699-pdc";
		reg = <0x21>;
		irq-gpios = <&gpio6 2 GPIO_ACTIVE_LOW>;
		port-index-on-chip = <0x1>;
	};
};

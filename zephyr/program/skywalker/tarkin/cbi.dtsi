/ {
	tarkin-fw-config {
		compatible = "cros-ec,cbi-fw-config";

		usbc-ports {
			enum-name = "USBC_PORTS";
			start = <8>;
			size = <2>;

			two-ports {
				compatible = "cros-ec,cbi-fw-config-value";
				enum-name = "TWO_PORTS";
				value = <0>;
			};
			one-port {
				compatible = "cros-ec,cbi-fw-config-value";
				enum-name = "ONE_PORT";
				value = <1>;
			};
		};
	};
};

/ {
	named-i2c-ports {
		compatible = "named-i2c-ports";

		i2c_ec_i2c_sensor_r: ec_i2c_sensor_r {
			i2c-port = <&i2c0>;
			enum-names = "I2C_PORT_SENSOR";
		};
	};
};

&i2c0 {
	status = "okay";
	dsp_client: dsp-client@21 {
		compatible = "cros,dsp-client";
		reg = <0x21>;
		/* TODO update gpio, this is a place holder*/
		int-gpios = <&gpio0 1 GPIO_ACTIVE_LOW>;
	};
};

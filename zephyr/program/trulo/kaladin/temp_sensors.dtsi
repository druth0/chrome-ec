 #include <cros/thermistor/thermistor.dtsi>

 / {
	 temp_io_port_thermistor: temp_io_port {
		 compatible = "cros-ec,temp-sensor-thermistor";
		 thermistor = <&thermistor_3V3_30K9_47K_4050B>;
		 adc = <&adc_temp_sensor_1>;
	 };
	 temp_cpu_thermistor: temp_cpu {
		compatible = "cros-ec,temp-sensor-thermistor";
		thermistor = <&thermistor_3V3_30K9_47K_4050B>;
		adc = <&adc_temp_sensor_2>;
	};
	 temp_board_thermistor: temp_board {
		compatible = "cros-ec,temp-sensor-thermistor";
		thermistor = <&thermistor_3V3_30K9_47K_4050B>;
		adc = <&adc_temp_sensor_3>;
	};

	named-temp-sensors {
		compatible = "cros-ec,temp-sensors";
		temp_io_port: io_port {
			temp-host-high = <85>;
			temp-host-halt = <85>;
			temp-host-release-high = <85>;
			power-good-pin = <&gpio_ec_soc_dsw_pwrok>;
			sensor = <&temp_io_port_thermistor>;
		};

		temp_cpu: cpu {
			temp-host-high = <85>;
			temp-host-halt = <85>;
			temp-host-release-high = <85>;
			power-good-pin = <&gpio_ec_soc_dsw_pwrok>;
			sensor = <&temp_cpu_thermistor>;
		};

		temp_board: board {
			temp-host-high = <85>;
			temp-host-halt = <85>;
			temp-host-release-high = <85>;
			power-good-pin = <&gpio_ec_soc_dsw_pwrok>;
			sensor = <&temp_board_thermistor>;
		};
	};
};

&thermistor_3V3_30K9_47K_4050B {
	status = "okay";
};

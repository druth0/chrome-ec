/ {
	pwmleds {
		compatible = "pwm-leds";
		pwm_power_led: pwm_power_led {
			pwms = <&pwm4 0 PWM_HZ(324) PWM_POLARITY_INVERTED>;
		};
		pwm_battery_amber_led: pwm_battery_amber_led {
			pwms = <&pwm1 0 PWM_HZ(324) PWM_POLARITY_INVERTED>;
		};
		pwm_battery_white_led: pwm_battery_white_led {
			pwms = <&pwm2 0 PWM_HZ(324) PWM_POLARITY_INVERTED>;
		};
	};
};

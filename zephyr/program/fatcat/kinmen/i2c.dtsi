&i2c1 {
	pinctrl-0 = <&i2c1_clk_gpc1_default
		     &i2c1_data_gpc2_default>;
	pinctrl-names = "default";

	label = "I2C_PORT_PD";
	clock-frequency = <I2C_BITRATE_STANDARD>;
};

&i2c4 {
	pinctrl-0 = <&i2c4_clk_gpe0_default
		     &i2c4_data_gpe7_default>;
	pinctrl-names = "default";

	label = "I2C_PORT_CHARGER";
	clock-frequency = <I2C_BITRATE_STANDARD>;
};

&i2c5 {
	pinctrl-0 = <&i2c5_clk_gpe1_default
		     &i2c5_data_gpe2_default>;
	pinctrl-names = "default";

	scl-gpios = <&gpioe 1 0>;
	sda-gpios = <&gpioe 2 0>;

	label = "I2C_PORT_SENSOR";
	clock-frequency = <I2C_BITRATE_FAST>;
	dsp_service: dsp-service@20 {
		compatible = "cros,dsp-service";
		reg = <0x20>;
		int-gpios = <&gpioh 6 (GPIO_ACTIVE_LOW)>;
	};
};

&i2c1 {
	status = "okay";
};

&i2c4 {
	status = "okay";
};

&i2c5 {
	status = "okay";
};

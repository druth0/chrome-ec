/ {
	/*
	 * These pin definitions should have been auto-generated,
	 * but the tooling did not catch them.
	 */

	named-gpios {
		compatible = "named-gpios";

		gpio_gpf3: gpf3 {
			gpios = <&gpiof 3 (GPIO_INPUT)>;
		};
	};
};

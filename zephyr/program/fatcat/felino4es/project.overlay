/* fatcat program common DTS includes */

#include <cros/integrated_fwid.dtsi>

/* felino4es project DTS includes */
#include "../felino/gpios.dtsi"

#include "../felino/adc.dtsi"
#include "../felino/binman.dtsi"
#include "../felino/console.dtsi"
#include "../felino/fan.dtsi"
#include "../felino/i2c.dtsi"
#include "../felino/interrupts.dtsi"
#include "keyboard.dtsi"
#include "../felino/led_pins.dtsi"
#include "../felino/led_policy.dtsi"
#include "../felino/power_signals.dtsi"
#include "../felino/temp_sensors.dtsi"
#include "../felino/usb_typec.dtsi"

/ {
	batteries {
		default_battery: smp-ap23b7q {
			compatible = "smp,ap23b7q", "battery-smart";
		};
		panasonic_ap23a5l_2 {
			compatible = "panasonic,ap23a5l-2", "battery-smart";
		};
		lges-ap22a8n {
			compatible = "lges,ap22a8n", "battery-smart";
		};
		smp_ap23a7l {
			compatible = "smp,ap23a7l", "battery-smart";
		};
		lges-ap23a8l {
			compatible = "lges,ap23a8l", "battery-smart";
		};
	};

	usba-port-enable-list {
		compatible = "cros-ec,usba-port-enable-pins";
		enable-pins = <&gpio_en_pp5000_usba>;
	};
};

/* AP interface */
&espi0 {
	status = "okay";
};

&sha0 {
	status = "okay";
};

/ {
	aliases {
		i2c-0 = &i2c0;
	};

	named-i2c-ports {
		compatible = "named-i2c-ports";
		i2c_ec_i2c_sensor: sensor {
			i2c-port = <&i2c2>;
			enum-names = "I2C_PORT_SENSOR";
		};
	};
};

&i2c2 {
	status = "okay";
	dsp_client: dsp-client@20 {
		compatible = "cros,dsp-client";
		reg = <0x20>;
		int-gpios = <&gpio0 1 GPIO_ACTIVE_LOW>;
	};
};

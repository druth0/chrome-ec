/ {
	named-i2c-ports {
		compatible = "named-i2c-ports";

		i2c_charger: charger {
			i2c-port = <&charger_i2c>;
			enum-names = "I2C_PORT_CHARGER";
		};
		ec_pd_power: typec-aic1-power {
			i2c-port = <&pd_pow_i2c>;
			enum-names = "I2C_PORT_PD";
		};
	};
};

/* charger */
charger_i2c: &i2c_smb_0 {
	label = "I2C_CHARGER";
	clock-frequency = <I2C_BITRATE_STANDARD>;
	pinctrl-0 = <&i2c00_scl_gpio004 &i2c00_sda_gpio003>;
	pinctrl-names = "default";
	port-sel = <0>;
	status = "okay";
};

/* typec-aic1-power */
pd_pow_i2c: &i2c_smb_1 {
	label = "I2C_PD_POWER";
	clock-frequency = <I2C_BITRATE_STANDARD>;
	pinctrl-0 = <&i2c06_scl_gpio140 &i2c06_sda_gpio132>;
	pinctrl-names = "default";
	port-sel = <6>;
	status = "okay";
};

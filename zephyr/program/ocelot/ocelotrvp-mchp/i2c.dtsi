/* charger */
i2c_port_charger: &i2c_smb_0 {
	pinctrl-0 = <&i2c00_scl_gpio004 &i2c00_sda_gpio003>;
	pinctrl-names = "default";
	port-sel = <0>;
	status = "okay";

	it8801_mfd: it8801@39 {
		compatible = "ite,it8801-mfd";
		reg = <0x39>;
	};
};

/* typec-aic1-power */
i2c_port_pd1: &i2c_smb_1 {
	pinctrl-0 = <&i2c06_scl_gpio140 &i2c06_sda_gpio132>;
	pinctrl-names = "default";
	port-sel = <6>;
	status = "okay";
};

/ {
	i2c_port_pd2: placeholder_i2c_port_pd2 {

	};
	i2c_port_ish: placeholder_i2c_port_ish {

	};
};

#include "../rvp_i2c.dtsi"

/delete-node/ &i2c_named_i2c_port_pd2;
/delete-node/ &i2c_named_i2c_port_ish;

/* In the board-specific overlay, apply the `tach_fan0` nodelabel to the
 * appropriate tachometer node, apply the `gpio_fan_control` and
 * `gpio_all_sys_pwr_good` labels to the appropriate named-gpio nodes, and set
 * the fan node's `pwms` property.
 */

/ {
	fans {
		compatible = "cros-ec,fans";
		fan0: fan0 {
			/* Provide pwms in board overlay */
			tach = <&tach_fan0>;
			rpm-min = <3000>;
			rpm-start = <3000>;
			rpm-max = <9000>;
			rpm-deviation = <1>;
			enable-gpio = <&gpio_fan_control>;
			pgood-gpio = <&gpio_all_sys_pwr_good>;
		};
	};
};
